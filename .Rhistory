barrera$T1T2T3 <- case_when(
barrera$T1_treat == 1 ~ 1,
barrera$T2_treat == 1 ~ 2,
barrera$T3_treat == 1 ~ 3,
barrera$T1_treat == 0 & barrera$T2_treat == 0 & barrera$T3_treat == 0 ~ 0
)
